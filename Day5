#include<iostream>
#include<bits/stdc++.h>
using namespace std;

struct card{
    int face;
    int shape;
    int color;

};
int main(){
    struct card c;
    c.face=1;
    c.shape=0;
    c.color=0;
    
    //c.struct card c={1,0,0};

    cout<<c.color;
    cout<<c.face;
    cout<<c.shape;

    return 0;
}
